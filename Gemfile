  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'therubyracer'
  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
gem 'chosen-rails'

group :development do
  gem 'pry-rails'
  gem 'sqlite3'
  gem 'debugger'
end

